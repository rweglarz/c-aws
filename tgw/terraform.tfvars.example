bootstrap_options = {
  common = {
    cgname                      = "cgm"
    dhcp-accept-server-hostname = "yes"
    dgname                      = "aws-gwlb"
    tplname                     = "aws-gwlb"
    mgmt-interface-swap         = "enable"
  }
  pan_pub = {
    panorama-server   = "aaa.bbb.ccc.ddd"
  }
  gwlb = {
    plugin-op-commands = "aws-gwlb-inspect:enable"
    vm-auth-key        = "aaaaaaaaaaaaaaa"
    authcodes          = "D0000000"
  }
  redis = {
    plugin-op-commands = "aws-gwlb-inspect:enable,set-sess-ress:True"
    redis-endpoint = "0000000000000.serverless.euc1.cache.amazonaws.com:6379"
    redis-certificate = ""
  }
}

# these two should go together most of the time
target_failover = "rebalance"
use_redis       = true


